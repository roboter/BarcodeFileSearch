<Window x:Class="BarcodeFileSearch.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BarcodeFileSearch"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="447" TextChanged="textBox_TextChanged"/>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="271" Margin="10,38,0,0" VerticalAlignment="Top" Width="497" MouseDoubleClick="listBox_MouseDoubleClick"/>
        <Button x:Name="button" Content="Clear" HorizontalAlignment="Left" Margin="462,13,0,0" VerticalAlignment="Top" Width="45" Click="button_Click"/>

    </Grid>
</Window>
